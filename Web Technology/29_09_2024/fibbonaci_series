const prompt = require("prompt-sync")();

const count = parseInt(prompt('Enter How many digit of febonacci series you want: '));

let n1 = 0; 
let n2 = 1; 
let n3 = 0;
console.log(n1)
console.log(n2)

for (let i =2; i< count; i++){
    n3 = n2 +n1;
    console.log(n3+" ")
    n1= n2;
    n2 = n3;
}
